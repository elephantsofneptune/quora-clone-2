


<div class="topic-select-wrapper group">
  <h2 class="page-header"> Follow Topics </h2>
  <h4 class="error-message page-subheader"> (Select at Least 3) </h4>
  <% topics.each( function (topic) { %>
    <div class="topic-wrapper">
    <input
      class="topic-option"
      type="checkbox"
      value="<%= topic.escape('id') %>"
      <% if (_.contains(ids, topic.escape('id'))) { %>
      checked
      <% } %>
    >
      <%= topic.escape('subject') %>
    </div>
  <% }) %>

  <button class="primary-button submit-topics">SUBMIT</button>
</div>
